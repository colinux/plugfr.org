<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>PLUG &#8211; Provence Linux User Group</title>
 <link href="http://plugfr.org/atom.xml" rel="self"/>
 <link href="http://plugfr.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://plugfr.org/</id>
 <author>
   <name>PLUG &#8211; Provence Linux User Group</name>
   <email>contact@plugfr.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://plugfr.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://plugfr.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>

